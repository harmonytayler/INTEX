<!-- Second landing page, once someone has logged in. -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Main Landing Page</title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <main>
        <section>
            <!-- Button Forms -->
            <form action="/users" method="GET">
                <button type="submit">Team Members</button>
            </form>
            <form action="/showVolunteers" method="GET">
                <button type="submit">Volunteer Requests</button>
            </form>
            <form action="/eventRequests" method="GET">
                <button type="submit">Event Requests</button>
            </form>
            <form action="/scheduledEvents" method="GET">
                <button type="submit">Scheduled Events</button>
            </form>
            <form action="/pastEvents" method="GET">
                <button type="submit">Past Events</button>
            </form>
        </section>
    </main>
</body>
</html>


<!-- Buttons to maintain users, volunteers, event requests, scheduled events, and past events -->
